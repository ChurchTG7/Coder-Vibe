<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Plugin>False</Plugin>
  <PluginURL/>
  <PluginAuthor/>
  <Name>Coder</Name>
  <Description>Cloud-based development environment for VS Code and JetBrains IDEs</Description>
  <Registry>https://ghcr.io/coder/coder</Registry>
  <Repository>ghcr.io/coder/coder:v2.27.9-amd64</Repository>
  <Tag>v2.27.9-amd64</Tag>
  <Project>https://coder.com</Project>
  <Category>Development:IDE</Category>
  <Icon>https://raw.githubusercontent.com/coder/coder/main/site/static/logo.svg</Icon>
  <Support>https://forums.unraid.net/</Support>
  <Overview>Coder is a platform for provisioning self-hosted cloud development environments. Deploy VS Code or JetBrains IDEs in your web browser.</Overview>
  <WebUI>http://[IP]:[PORT:7080]</WebUI>
  <Template>
    <Required/>
    <Privileged>False</Privileged>
    <Network type="bridge">bridge</Network>
    <TerminalRoot>/</TerminalRoot>
    <Shell>sh</Shell>
    <Logging/>
    <Volumes>
      <Volume Name="Coder Config" HostDir="/mnt/user/appdata/coder" ContainerDir="/home/coder" Mode="rw"/>
      <Volume Name="Coder Data" HostDir="/mnt/user/appdata/coder/data" ContainerDir="/var/lib/coder" Mode="rw"/>
    </Volumes>
    <Ports>
      <Port Name="Coder Web UI" HostPort="7080" ContainerPort="7080" Protocol="tcp"/>
    </Ports>
    <Variables>
      <Variable Name="CODER_HTTP_ADDRESS" Value="0.0.0.0:7080"/>
      <Variable Name="CODER_TLS_ENABLE" Value="false"/>
      <Variable Name="CODER_SECURE_AUTH_COOKIE" Value="false"/>
      <Variable Name="CODER_POSTGRESQL_URL" Value="file:///var/lib/coder/coder.db"/>
      <Variable Name="CODER_PROVISIONER_DAEMONS" Value="1"/>
      <Variable Name="CODER_ALLOW_PATH_APP_SHARING" Value="true"/>
      <Variable Name="CODER_ALLOW_DEV_URLS" Value="true"/>
      <Variable Name="CODER_DISABLE_SESSION_EXPIRY" Value="false"/>
    </Variables>
    <Labels>
      <Label>Coder</Label>
      <Label>IDE</Label>
      <Label>Development</Label>
      <Label>VSCode</Label>
    </Labels>
    <Beta>False</Beta>
    <ReadOnly>False</ReadOnly>
  </Template>
</Containers>
